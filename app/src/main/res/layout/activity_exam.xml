<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:background="@color/text" tools:context=".Frag.FragMain">
    <View
        android:layout_width="393dp"
        android:layout_height="1dp"
        android:background="@color/white"
        android:layout_gravity="bottom" />
    <FrameLayout
        android:layout_width="313dp"
        android:layout_height="match_parent"
        android:layout_marginTop="26dp"
        android:layout_marginBottom="44dp"
        android:layout_marginLeft="33dp"
        android:layout_marginRight="34dp"
        android:background="#edf1f6"></FrameLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="44dp"
        android:layout_marginLeft="23dp"
        android:layout_marginRight="24dp"
        android:background="@color/white">
            <ImageView
            android:layout_width="290dp"
            android:layout_height="121dp"
            android:id="@+id/imageView_O"
            android:src="@drawable/correct"
            android:visibility="invisible"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/button"
            android:layout_above="@+id/linearLayout"
            android:layout_marginTop="60dp"
            android:layout_marginLeft="20dp"
            />
        <ImageView
            android:layout_width="290dp"
            android:layout_height="121dp"
            android:id="@+id/imageView_X"
            android:src="@drawable/incorrect"
            android:visibility="invisible"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/button"
            android:layout_above="@+id/linearLayout"
            android:layout_marginTop="60dp"
            android:layout_marginLeft="20dp" />
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="14dp"
            android:id="@+id/linearLayout4">
            <ImageView
                android:layout_width="19dp"
                android:layout_height="19dp"
                android:layout_marginLeft="19dp"
                android:layout_marginRight="9dp"
                android:src="@drawable/cardnumber"
                android:layout_gravity="center"
                android:gravity="center"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/now"
                android:textSize="13.8sp"
                android:textColor="#565657"
                android:lineSpacingExtra="-3.6sp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="01"/>

            <TextView
                android:text=" / "
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView5"
                android:textSize="13.8sp"
                android:textColor="#565657"
                android:lineSpacingExtra="-3.6sp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/amount"
                android:textSize="13.8sp"
                android:textColor="#565657"
                android:lineSpacingExtra="-3.6sp"
                android:text="10"/>
            <ImageView
                android:layout_width="19dp"
                android:layout_height="19dp"
                android:src="@drawable/colck"
                android:id="@+id/pic_timer"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginLeft="60dp"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/pic_time"
                android:textSize="13.8sp"
                android:textColor="#565657"
                android:lineSpacingExtra="-3.6sp"
                android:paddingLeft="5dp"
                android:text="5"/>
            <ImageButton
                android:layout_width="19dp"
                android:layout_height="19dp"
                android:layout_marginLeft="60dp"
                android:src="@drawable/add"
                android:layout_gravity="center"
                android:gravity="center"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="8dp"
                android:textSize="13.8sp"
                android:textColor="#565657"
                android:lineSpacingExtra="-3.6sp"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="단어장 추가"
                android:id="@+id/add_button"/>
        </LinearLayout>

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="83dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="61sp"
                android:id="@+id/textView"
                android:layout_gravity="center"
                android:gravity="center"
                android:textSize="41.5sp"
                android:textColor="#209cd8"
                android:text="Advertise"
                />
            <TextView
                android:layout_width="194dp"
                android:layout_height="40.5dp"
                android:id="@+id/textView1"
                android:textSize="19.7sp"
                android:textColor="#565656"
                android:lineSpacingExtra="-5.1sp"
                android:text="test1"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_marginTop="36dp"
                android:layout_marginBottom="14dp"
                android:background="@drawable/word_button"/>
            <TextView
                android:layout_width="194dp"
                android:layout_height="40.5dp"
                android:id="@+id/textView2"
                android:textSize="19.7sp"
                android:textColor="#565656"
                android:lineSpacingExtra="-5.1sp"
                android:text="test2"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/word_button"
                android:layout_marginBottom="14dp"/>
            <TextView
                android:layout_width="194dp"
                android:layout_height="40.5dp"
                android:id="@+id/textView3"
                android:textSize="19.7sp"
                android:textColor="#565656"
                android:lineSpacingExtra="-5.1sp"
                android:text="test3"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/word_button"
                android:layout_marginBottom="14dp"/>
            <TextView
                android:layout_width="194dp"
                android:layout_height="40.5dp"
                android:id="@+id/textView4"
                android:textSize="19.7sp"
                android:textColor="#565656"
                android:lineSpacingExtra="-5.1sp"
                android:text="test4"
                android:layout_gravity="center"
                android:gravity="center"
                android:background="@drawable/word_button"
                android:layout_marginBottom="14dp"/>
            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="다음 >"
                android:id="@+id/button"
                android:textSize="14.5sp"
                android:lineSpacingExtra="-2.2sp"
                android:background="#00ff0000"
                android:textColor="@color/text"
                android:visibility="visible"
                android:layout_alignParentBottom="true"
                android:layout_gravity="center"
                android:gravity="center"/>
        </LinearLayout>
    </FrameLayout>
</RelativeLayout>
